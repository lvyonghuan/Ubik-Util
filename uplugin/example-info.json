{
  "name": "ExamplePlugin",
  "description": "This is an example plugin for testing purposes",
  "version": "1.0.0",
  "author": "Ubik",
  "Uri": "http://localhost:24242",
  "Local": true,
  "nodes": {
    "StartNode": {
      "info": "This is the beginning node of the workflow",
      "is_begin": true,
      "is_special_begin": false,
      "input": {},
      "output": {
        "text": {
          "attribute": "string",
          "type": "text",
          "info": "Text output from start node",
          "streaming": false
        },
        "number": {
          "attribute": "numeric",
          "type": "integer",
          "info": "Numeric output",
          "streaming": false
        }
      },
      "params": {
        "initialValue": {
          "attribute": "config",
          "type": "text",
          "info": "Initial value to process",
          "streaming": false
        }
      }
    },
    "ProcessNode": {
      "info": "Processes data from the start node",
      "is_begin": false,
      "input": {
        "inputText": {
          "attribute": "string",
          "type": "text",
          "info": "Text to process",
          "streaming": false
        },
        "inputNumber": {
          "attribute": "numeric",
          "type": "integer",
          "info": "Number to process",
          "streaming": false
        }
      },
      "output": {
        "processedData": {
          "attribute": "object",
          "type": "json",
          "info": "Processed data output",
          "streaming": true
        }
      },
      "params": {
        "processType": {
          "attribute": "option",
          "type": "select",
          "info": "Type of processing to apply",
          "streaming": false
        }
      }
    },
    "OutputNode": {
      "info": "Final node that outputs the results",
      "is_begin": false,
      "input": {
        "data": {
          "attribute": "object",
          "type": "json",
          "info": "Data to output",
          "streaming": true
        }
      },
      "output": {},
      "params": {
        "format": {
          "attribute": "option",
          "type": "select",
          "info": "Output format",
          "streaming": false
        },
        "destination": {
          "attribute": "string",
          "type": "text",
          "info": "Output destination",
          "streaming": false
        }
      }
    }
  }
}
